<UserControl x:Class="I2MS2.UserControls.Drawing.DrawingItem2DRack"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="20" d:DesignWidth="100">
    <Grid>
        <StackPanel Orientation="Horizontal">
            <Rectangle Name="_rectRack" Width="12" Height="12" VerticalAlignment="Center" Fill="SkyBlue"/>
            <TextBlock Name="_txtName" Text="Rack" Style="{StaticResource I2MS_TextBlockStyle}" VerticalAlignment="Center" Margin="5,0,0,0"/>
        </StackPanel>
    </Grid>
</UserControl>
